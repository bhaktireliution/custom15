<?xml version="1.0" encoding="utf-8"?>
<odoo>

       <record id="view_order_tree" model="ir.ui.view">
            <field name="name">real_estate.order.tree</field>
            <field name="model">real_estate.order</field>
            <field name="arch" type="xml">
                <tree string="Properties" >
                    <field name="name" string="Title" readonly="1"/>
                    <field name="post_code" string="Postcode"/>
                    <field name="Bedrooms" readonly="1"/>
                    <field name="Living Area(sqm)"/>
                    <field name="Expected Price" readonly="1" />
                    <field name="Selling Price" readonly="1" />
                    <field name="from_date" string="Available From" widget="date"/>
                </tree>
            </field>
       </record>

